{
    "Number Calculation Mod": {
        "prefix": "methodMod",
        "body": [
          "int add (int a, int b) {",
          "  return (a += b) >= MOD ? a - MOD : a;",
          "}",
          " ",
          "int sub (int a, int b) {",
          "  return (a -= b) < 0 ? a + MOD : a;",
          "}",
          " ",
          "int mul (int a, int b) {",
          "  return (long long) a * b % MOD;",
          "}",
          " ",
          "int power(int a, long long n) {",
          "  int res = 1;",
          "  while(n) {",
          "    if (n & 1LL) res = mul(res, a);",
          "    a = mul(a, a);",
          "    n >>= 1LL;",
          "  }",
          "  return res;",
          "}",
          "int inv(int a) {",
          "  return power(a, MOD - 2);",
          "}",
          ""
        ],
        "description": "Number Calculation Mod"
      }
